<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.text.Text?>
<?import java.lang.String?>
<AnchorPane maxHeight="-Infinity" maxWidth="-Infinity" minHeight="-Infinity" minWidth="-Infinity"
            onMouseClicked="#onKey" prefHeight="400.0" prefWidth="600.0" styleClass="default-background"
            stylesheets="@../styles.css" xmlns="http://javafx.com/javafx/21" xmlns:fx="http://javafx.com/fxml/1"
            fx:controller="de.uniks.stp24.controller.GameLaunchController">
    <VBox prefHeight="200.0" prefWidth="100.0" AnchorPane.bottomAnchor="0.0" AnchorPane.leftAnchor="0.0"
          AnchorPane.rightAnchor="0.0" AnchorPane.topAnchor="0.0">
        <HBox prefHeight="100.0" prefWidth="200.0"/>
        <HBox prefHeight="100.0" prefWidth="200.0" VBox.vgrow="ALWAYS">
            <AnchorPane prefHeight="200.0" prefWidth="200.0" HBox.hgrow="ALWAYS">
                <VBox alignment="CENTER" fillWidth="false" prefHeight="200.0" prefWidth="100.0"
                      AnchorPane.bottomAnchor="20.0" AnchorPane.leftAnchor="20.0" AnchorPane.rightAnchor="20.0"
                      AnchorPane.topAnchor="20.0">
                    <VBox alignment="CENTER" fillWidth="false" prefHeight="220.0" prefWidth="220.0"
                          styleClass="round-image-container">
                        <ImageView fx:id="phoenixImageView" fitHeight="220.0" fitWidth="350.0" pickOnBounds="true"
                                   preserveRatio="true"/>
                    </VBox>
                </VBox>
            </AnchorPane>
            <AnchorPane prefHeight="200.0" prefWidth="200.0" HBox.hgrow="ALWAYS">
                <VBox alignment="CENTER" fillWidth="false" prefHeight="200.0" prefWidth="100.0"
                      AnchorPane.bottomAnchor="20.0" AnchorPane.leftAnchor="20.0" AnchorPane.rightAnchor="20.0"
                      AnchorPane.topAnchor="20.0">
                    <VBox alignment="CENTER" fillWidth="false" prefHeight="220.0" prefWidth="220.0"
                          styleClass="round-image-container">
                        <ImageView fx:id="deadBirdsImageView" fitHeight="220.0" fitWidth="350.0" pickOnBounds="true"
                                   preserveRatio="true"/>
                    </VBox>
                </VBox>
            </AnchorPane>
        </HBox>
        <HBox alignment="TOP_CENTER" prefHeight="100.0" prefWidth="200.0">
            <VBox alignment="CENTER" maxHeight="-Infinity" prefHeight="20.0" prefWidth="100.0">
                <styleClass>
                    <String fx:value="container"/>
                    <String fx:value="dark-container"/>
                </styleClass>
                <Text fx:id="pressKeyText" strokeType="OUTSIDE" strokeWidth="0.0" styleClass="large"
                      text="%press.any.Key" textAlignment="CENTER"/>
            </VBox>
        </HBox>
    </VBox>
</AnchorPane>
