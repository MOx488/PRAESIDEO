<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.*?>
<?import java.lang.String?>
<fx:root maxHeight="-Infinity" maxWidth="-Infinity" minHeight="-Infinity" minWidth="-Infinity" prefWidth="367.0"
         stylesheets="@../../styles.css" type="AnchorPane" xmlns="http://javafx.com/javafx/21"
         xmlns:fx="http://javafx.com/fxml/1" fx:controller="de.uniks.stp24.component.popups.DeleteGamePopUpComponent">
    <VBox prefHeight="178.0" prefWidth="367.0" AnchorPane.bottomAnchor="0.0" AnchorPane.leftAnchor="0.0"
          AnchorPane.rightAnchor="0.0" AnchorPane.topAnchor="0.0">
        <VBox prefHeight="6.0" prefWidth="367.0" style="-fx-border-color: black; -fx-border-width: 1;">
            <padding>
                <Insets bottom="10.0" left="10.0" right="10.0" top="10.0"/>
            </padding>
            <Label styleClass="medium" text="%delete.game"/>
        </VBox>
        <VBox alignment="TOP_CENTER" prefWidth="361.0">
            <padding>
                <Insets bottom="10.0" left="10.0" right="10.0" top="10.0"/>
            </padding>
            <Label styleClass="medium" text="%are.you.sure"/>
            <HBox alignment="CENTER" prefHeight="13.0" prefWidth="367.0">
                <VBox prefHeight="19.0" prefWidth="184.0" HBox.hgrow="ALWAYS">
                    <padding>
                        <Insets bottom="20.0" left="20.0" right="20.0" top="20.0"/>
                    </padding>
                    <Button fx:id="cancelButton" cancelButton="true" mnemonicParsing="false" onAction="#cancel"
                            styleClass="back-button" text="%cancel"/>
                </VBox>
                <VBox alignment="TOP_RIGHT" prefHeight="17.0" prefWidth="141.0" HBox.hgrow="ALWAYS">
                    <padding>
                        <Insets bottom="20.0" left="20.0" right="20.0" top="20.0"/>
                    </padding>
                    <Button fx:id="deleteGameButton" defaultButton="true" mnemonicParsing="false" onAction="#deleteGame"
                            styleClass="back-button" text="%yes"/>
                </VBox>
            </HBox>
        </VBox>
        <styleClass>
            <String fx:value="container"/>
            <String fx:value="dark-container"/>
        </styleClass>
    </VBox>
</fx:root>
