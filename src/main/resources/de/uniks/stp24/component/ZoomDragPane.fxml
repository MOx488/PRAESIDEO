<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.canvas.Canvas?>
<?import javafx.scene.control.ScrollPane?>
<?import javafx.scene.Group?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.StackPane?>
<fx:root type="StackPane" xmlns="http://javafx.com/javafx/21" xmlns:fx="http://javafx.com/fxml/1"
         fx:controller="de.uniks.stp24.component.ZoomDragComponent">
    <ScrollPane fx:id="scrollPane">
        <Group fx:id="target">
            <ImageView fx:id="view" cache="true" pickOnBounds="true"/>
            <Canvas fx:id="canvas"/>
            <Group fx:id="castleContainer"/>
        </Group>
    </ScrollPane>
</fx:root>
